  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "prebuild": "npm run test",
    "clean": "rimraf dist",
    "dev": "cross-env NODE_ENV=development nodemon -r tsconfig-paths/register src/index.ts",
    "staging": "cross-env NODE_ENV=staging node dist/index.js",
    "prod": "cross-env NODE_ENV=production node dist/index.js",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "jest",
    "test:unit": "cross-env TEST_TYPE=unit jest tests/unit --coverage",
    "test:integration": "cross-env TEST_TYPE=integration jest tests/integration --coverage",
    "test:e2e": "cross-env TEST_TYPE=e2e jest tests/e2e --coverage",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e:service": "cross-env NODE_ENV=development nodemon -r tsconfig-paths/register src/e2e.service.ts",
    "lint": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "lint:fix": "prettier --write \"src/**/*.{ts,js,json,md}\""
  },
